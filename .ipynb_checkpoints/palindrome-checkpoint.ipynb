   "source": [
    "def palindrome(n):\n",
    "    temp=n\n",
    "    rotate=0\n",
    "    while(temp!=0):\n",
    "        rotate=rotate*10+temp%10\n",
    "        temp=temp//10\n",
    "    if(rotate==n):\n",
    "        print(f\"{n} is palindrome\")\n",
    "    else:\n",
    "        print(f\"{n} is not palindrome\")"
   ]
